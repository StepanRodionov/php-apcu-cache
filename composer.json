{
  "name": "stepanrodionov/php-apcu-cache",
  "description": "This is PSR-6 compatible cache library using php-apcu module",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Rodionov Stepan",
      "email": "stepanhero111@gmail.com"
    },
    {
      "name": "Panychev Dmitry",
      "email": "thor_work@yahoo.com"
    }
  ],
  "require": {
    "php": "^7.0",
    "ext-apcu": "*",
    "psr/simple-cache": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "SR\\ApcuSimpleCache\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SR\\ApcuSimpleCacheTest\\": "test"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "require-dev": {
    "phpunit/phpunit": "^7"
  }
}
